SELECT guid, post_title                                     FROM efl_posts                                     INNER JOIN efl_postmeta ON efl_posts.ID = efl_postmeta.post_id                                      INNER JOIN efl_postmeta aux ON efl_postmeta.post_id = aux.post_id                                     WHERE ( post_content LIKE '%Memento Fiscal 2012%' OR                                              post_title LIKE '%{$query}%' OR (                                              efl_postmeta.meta_key = '_obras' AND                                             efl_postmeta.meta_value LIKE '%Memento Fiscal 2012%' )                                             ) AND aux.meta_key = '_p_prot' AND aux.meta_value = 'off'                                             ORDER BY post_date DESC;
SELECT Distinct(guid), post_title                                     FROM efl_posts                                     INNER JOIN efl_postmeta ON efl_posts.ID = efl_postmeta.post_id                                      INNER JOIN efl_postmeta aux ON efl_postmeta.post_id = aux.post_id                                     WHERE ( post_content LIKE '%Memento Fiscal 2012%' OR                                              post_title LIKE '%{$query}%' OR (                                              efl_postmeta.meta_key = '_obras' AND                                             efl_postmeta.meta_value LIKE '%Memento Fiscal 2012%' )                                             ) AND aux.meta_key = '_p_prot' AND aux.meta_value = 'off'                                             ORDER BY post_date DESC;
