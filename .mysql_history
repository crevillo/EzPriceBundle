SELECT post_title, ID, guid from efl_posts  
                                    INNER JOIN efl_postmeta
                                    ON efl_posts.ID = efl_postmeta.post_id
                                    WHERE ( post_content LIKE '%Memento Contrato de Trabajo 2012-2013%' OR
                                          post_title LIKE '%Memento Contrato de Trabajo 2012-2013%' OR ( meta_key = '_obras' AND 
                                            meta_value LIKE '%Memento Contrato de Trabajo 2012-2013%' ) 
                                          ) AND meta_key = '_p_prot' AND meta_value = 'off';
