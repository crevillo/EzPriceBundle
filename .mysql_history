CREATE TABLE leads (
  ID int(10) unsigned NOT NULL auto_increment,
  EMPRESA varchar(100) default '',
  NOMBRE varchar(100) default '',
  APELLIDO1 varchar(100) default '',
  APELLIDO2 varchar(100) default '',
  EMAIL varchar(100) default '',
  CIF varchar(50) default '',
  TELEFONO varchar(50) default '',
  MOVIL varchar(50) default '',
  CODIGO_POSTAL varchar(5) default '',
  LOCALIDAD varchar(100) default '',
  PROVINCIA varchar(100) default '',
  CODIGO_PRODUCTO varchar(50) default '',
  NOMBRE_PRODUCTO varchar(100) default '',
  ACCION_COMERCIAL varchar(50) default '',
  COLECTIVO varchar(50) default '',
  PROFESION varchar(50) default '',
  CARGO varchar(50) default '',
  DEPARTAMENTO varchar(50) default '',
  NUMERO_EMPLEADOS int(10) unsigned default '0',
  ACTIVIDAD varchar(50) default '',
  ESPECIALIDAD varchar(50) default '',
  AREAS_INTERES varchar(100) default '',
  PRIMARY KEY  (ID)
) TYPE=MyISAM;
CREATE TABLE leads (   ID int(10) unsigned NOT NULL auto_increment,   EMPRESA varchar(100) default '',   NOMBRE varchar(100) default '',   APELLIDO1 varchar(100) default '',   APELLIDO2 varchar(100) default '',   EMAIL varchar(100) default '',   CIF varchar(50) default '',   TELEFONO varchar(50) default '',   MOVIL varchar(50) default '',   CODIGO_POSTAL varchar(5) default '',   LOCALIDAD varchar(100) default '',   PROVINCIA varchar(100) default '',   CODIGO_PRODUCTO varchar(50) default '',   NOMBRE_PRODUCTO varchar(100) default '',   ACCION_COMERCIAL varchar(50) default '',   COLECTIVO varchar(50) default '',   PROFESION varchar(50) default '',   CARGO varchar(50) default '',   DEPARTAMENTO varchar(50) default '',   NUMERO_EMPLEADOS int(10) unsigned default '0',   ACTIVIDAD varchar(50) default '',   ESPECIALIDAD varchar(50) default '',   AREAS_INTERES varchar(100) default '',   PRIMARY KEY  (ID) ) TYPE=MyISAM;
