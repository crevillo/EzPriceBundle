CREATE TABLE IF NOT EXISTS `valoraciones_productos` (
  `user_id` int(11) NOT NULL,
  `node_producto` int(11) NOT NULL,
  `ha_votado` tinyint(255) DEFAULT NULL,
  `calidad` int(1) DEFAULT NULL,
  `actualizaciones` int(1) DEFAULT NULL,
  `facilidad` int(1) DEFAULT NULL,
  `comentario` text COLLATE utf8_bin,
  `visible` int(1) DEFAULT NULL,
  `fecha` int(11) NOT NULL,
  `nombre` text COLLATE utf8_bin NOT NULL,
  `apellidos` text COLLATE utf8_bin NOT NULL,
  `empresa` text COLLATE utf8_bin
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_bin;
