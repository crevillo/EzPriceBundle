CREATE TABLE IF NOT EXISTS `ezcustomdiscountsubrule` (
  `discount_percent` float default NULL,
  `discountrule_id` int(11) NOT NULL default '0',
  `id` int(11) NOT NULL auto_increment,
  `limitation` char(1) collate utf8_bin default NULL,
  `name` varchar(255) collate utf8_bin NOT NULL default '',
  `start` int(11) NOT NULL,
  `end` int(11) NOT NULL,
  PRIMARY KEY  (`id`)
) ENGINE=InnoDB  DEFAULT CHARSET=utf8 COLLATE=utf8_bin;
CREATE TABLE IF NOT EXISTS `ezcustomdiscountsubrule` (   `discount_percent` float default NULL,   `discountrule_id` int(11) NOT NULL default '0',   `id` int(11) NOT NULL auto_increment,   `limitation` char(1) collate utf8_bin default NULL,   `name` varchar(255) collate utf8_bin NOT NULL default '',   `start` int(11) NOT NULL,   `end` int(11) NOT NULL,   PRIMARY KEY  (`id`) ) ENGINE=InnoDB  DEFAULT CHARSET=utf8 COLLATE=utf8_bin;
