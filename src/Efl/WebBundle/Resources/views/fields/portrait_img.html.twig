{% block ezimage_field %}
{% spaceless %}
        {% if not ez_is_field_empty( content, field ) %}
            {% set imageAlias = ez_image_alias( field, versionInfo, parameters.alias|default( 'original' ) ) %}
            {% if imageAlias.uri is defined %}
            <img  class="{{ parameters.attrs.class }}" src="{{ asset( imageAlias.uri ) }}" alt="{{ field.value.alternativeText }}" />
            {% endif %}
        {% endif %}
{% endspaceless %}
{% endblock %}
